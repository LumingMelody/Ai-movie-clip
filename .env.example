# AI Model API Keys
DASHSCOPE_API_KEY=your_dashscope_api_key_here
OPENAI_API_KEY=your_openai_api_key_here
COZE_API_KEY=your_coze_api_key_here

# OSS Configuration (Alibaba Cloud)
OSS_ACCESS_KEY_ID=your_oss_access_key_id_here
OSS_ACCESS_KEY_SECRET=your_oss_access_key_secret_here
OSS_ENDPOINT=https://oss-cn-hangzhou.aliyuncs.com
OSS_BUCKET_NAME=your_bucket_name_here

# OSS Path Configuration
VIDEO_OSS_PATH=videos/original.mp4
ADS_OSS_FOLDER=ads/
OUTPUT_OSS_FOLDER=outputs/

# Other Cloud Services
ALIYUN_ACCESS_KEY_ID=your_aliyun_access_key_id_here
ALIYUN_ACCESS_KEY_SECRET=your_aliyun_access_key_secret_here

# Database Configuration (if needed)
DATABASE_URL=postgresql://user:password@localhost:5432/dbname

# Server Configuration
SERVER_HOST=0.0.0.0
SERVER_PORT=8000

# WebSocket Configuration
WEBSOCKET_HOST=10.211.55.3
WEBSOCKET_PORT=8888

# Feature Flags
USE_VOICE_CLONING=false
ENABLE_AUTO_INTRODUCE=true

# Voice Configuration
DEFAULT_VOICE_MODEL=cosyvoice-v2
DEFAULT_VOICE_TYPE=longxiaochun_v2

# Aliyun NLS (Natural Language Service) Configuration
NLS_URL=wss://nls-gateway-cn-shanghai.aliyuncs.com/ws/v1
NLS_TOKEN=your_nls_token_here
NLS_APPKEY=your_nls_appkey_here

# Local Temporary Directory
TEMP_DIR=./temp/

# User Data Directory (optional)
IKUN_DATA_DIR=./ikun